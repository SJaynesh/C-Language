#include <stdio.h>
#include <conio.h>

main()
{
	int n;
	float pi;
	char ch;
	int a = 12, b = 4;
	clrscr();

	printf("Enter any number : ");
	scanf("%d",&n);
	printf("Enter PI value : ");
	scanf("%f",&pi);
	fflush(stdin);
	printf("Enter any character : ");
	scanf("%c",&ch);  // O

	printf("\n\n");
	printf("Number\t: %d\n",n);
	printf("PI\t: %.2f\n",pi);
	printf("Character : %c\n\n",ch);

	// \n => New Line
	// \t => 8 Space
	printf("\\t \n\n");

	printf("%d %% %d = %d\n",a,b,a%b); // 12 % 4 = 0

	getch();
}